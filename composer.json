{
    "name": "bandama/app-lite",
    "description": "Bandama Application Skeleton with Monolog and Kahlan",
    "type": "project",
    "license": "MIT",
    "version": "1.2.0",
    "require-dev": {
        "php": ">=5.5",
        "kahlan/kahlan": "^3.1"
    },
    "require": {
        "php": ">=5.5",
        "bandama/framework": "1.2.*",
        "monolog/monolog": "^1.22"
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "spec/"
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": ["app/", "src/"],
            "App\\Core\\": "app/core/",
            "App\\Controller\\": "src/controllers/",
            "App\\Model\\": "src/models/",
            "App\\Service\\": "src/services/"
        }
    }
}